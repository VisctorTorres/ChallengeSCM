<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Demo</title>
    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />*@
    @*<link rel="stylesheet" href="~/css/site.css" />*@
    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="~/adminlte/plugins/fontawesome-free/css/all.min.css">

    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">

    <link rel="stylesheet" href="~/adminlte/plugins/fontawesome-free/css/all.min.css">
    <link href="~/adminlte/bootstrap-5.1.0-dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/adminlte/plugins/daterangepicker/daterangepicker.css" rel="stylesheet" />

    <!-- DataTables -->
    <link rel="stylesheet" href="~/adminlte/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="~/adminlte/plugins/datatables-responsive/css/responsive.bootstrap4.min.css">
    <link rel="stylesheet" href="~/adminlte/plugins/datatables-buttons/css/buttons.bootstrap4.min.css">
    <link href="~/adminlte/plugins/select2/css/select2.min.css" rel="stylesheet" />
    <link href="~/adminlte/plugins/select2-bootstrap4-theme/select2-bootstrap4.min.css" rel="stylesheet" />
    <link href="~/adminlte/bootstrap-5.1.0-dist/select2/css/select2-bootstrap-5-theme.min.css" rel="stylesheet" />
    <link href="~/adminlte/plugins/datatables-select/css/select.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/adminlte/plugins/datatables-fixedcolumns/css/fixedColumns.bootstrap4.min.css" rel="stylesheet" />
    <link href="~/adminlte/plugins/datatables-rowgroup/css/rowGroup.bootstrap4.min.css" rel="stylesheet" />
    <!-- sweetalert2 -->
    <link href="~/adminlte/plugins/sweetalert2-theme-bootstrap-4/bootstrap-4.min.css" rel="stylesheet" />
    <!-- Theme style -->
    <link rel="stylesheet" href="~/adminlte/dist/css/adminlte.min.css">
    <style>
        .loadmio {
            display: none;
            position: fixed;
            z-index: 1000;
            top: 0;
            left: 0;
            height: 100%;
            width: 100%;
            background: rgba( 255, 255, 255, .8 ) url('/img/ajax-loader7.gif') 50% 50% no-repeat;
        }

        /* When the body has the loading class, we turn
           the scrollbar off with overflow:hidden */

        body.loading {
            overflow: hidden;
        }

            /* Anytime the body has the loading class, our
           modal element will be visible */

            body.loading .loadmio {
                display: block;
            }
    </style>
    @RenderSection("Styles", required: false)
</head>
<body class="sidebar-mini control-sidebar-push">


    <div class="wrapper">
        <div class="modal loadmio"></div>
        <partial name="_Navbar" />
        <partial name="_Sidebar" />
        <partial name="_SidebarControl" />
        <div class="content-wrapper">
            <div class="content">
                <div class="container-fluid">

                    @RenderBody()

                </div>
            </div>

        </div>

        @*<partial name="_Footer" />*@
    </div>

    <!-- jQuery -->
    <script src="~/adminlte/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 5 -->
    <script src="~/adminlte/bootstrap-5.1.0-dist/js/bootstrap.bundle.js"></script>
    <!-- ChartJS -->
    <script src="~/adminlte/plugins/chart.js/Chart.min.js"></script>
    <!-- daterangepicker -->
    <script src="~/adminlte/plugins/moment/moment-with-locales.js"></script>
    <script src="~/adminlte/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- DataTables  & Plugins -->
    <script src="~/adminlte/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/adminlte/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/adminlte/plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/adminlte/plugins/datatables-responsive/js/responsive.bootstrap4.min.js"></script>
    <script src="~/adminlte/plugins/datatables-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/adminlte/plugins/datatables-buttons/js/buttons.bootstrap4.min.js"></script>
    <script src="~/adminlte/plugins/jszip/jszip.min.js"></script>
    <script src="~/adminlte/plugins/pdfmake/pdfmake.min.js"></script>
    <script src="~/adminlte/plugins/pdfmake/vfs_fonts.js"></script>
    <script src="~/adminlte/plugins/datatables-buttons/js/buttons.html5.min.js"></script>
    <script src="~/adminlte/plugins/datatables-buttons/js/buttons.print.min.js"></script>
    <script src="~/adminlte/plugins/datatables-buttons/js/buttons.colVis.min.js"></script>
    <script src="~/adminlte/plugins/select2/js/select2.min.js"></script>
    <script src="~/adminlte/plugins/datatables-select/js/dataTables.select.min.js"></script>
    <script src="~/adminlte/plugins/datatables-select/js/select.bootstrap4.min.js"></script>
    <script src="~/adminlte/plugins/datatables-fixedcolumns/js/dataTables.fixedColumnsLast.min.js"></script>

    <script src="~/adminlte/plugins/datatables-rowgroup/js/dataTables.rowGroup.min.js"></script>


    <!-- sweetalert2 -->
    <script src="~/adminlte/plugins/sweetalert2/sweetalert2.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/adminlte/dist/js/adminlte.min.js"></script>

    <script src="~/jsViews/AjaxLoading.js"></script>

    <script type="text/javascript">
        
    </script>
    @RenderSection("Scripts", required: false)
</body>
</html>
