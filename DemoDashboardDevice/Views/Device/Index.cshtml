@{
    ViewData["Title"] = "Terminales";
}
<section class="content">
    <div class="row" id="table_col_all">
        <div class="col-xs-12">
            <div class="card card-solid card-success">
                <div class="card-header with-border">
                    <h3 class="card-title">Terminales</h3>
                </div>
                <div class="card-body table-responsive" id="example_wrapper">
                    <table id="tbl_device" class="table table-bordered table-hover">
                        <thead>
                            <tr>
                                <th>Id</th>
                                <th>Nombre</th>
                                <th>Compañia</th>
                                <th>Acciones</th>
                            </tr>
                        </thead>
                        <tbody id="tbl_body_devices">
                            <!--Data por AJAX-->

                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>
@section Scripts
{
    <script src="@Url.Content("~/jsViews/Devices.js")?v=1"></script>
}